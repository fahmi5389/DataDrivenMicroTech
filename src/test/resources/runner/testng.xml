<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Data Driven Testing">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.mt.listeners.CustomListeners"/>
	</listeners>



	<test thread-count="5" name="Login Test">
		<classes>
			<class name="com.mt.testcases.BankManagerLoginTest" />
		</classes>
	</test>

	<test thread-count="5" name="Add Customer Test">
		<classes>
			<class name="com.mt.testcases.AddCustomerTest" />
		</classes>
	</test>

	<test thread-count="5" name="Open Account Test">
		<classes>
			<class name="com.mt.testcases.OpenAccountTest" />
		</classes>
	</test>



</suite> <!-- Suite -->
